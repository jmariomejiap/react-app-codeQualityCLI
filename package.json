{
  "name": "example-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "code-quality-cli": "git+https://github.com/jmariomejiap/codeQualityCLI.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage-report": "npm test -- --collectCoverage",
    "report-codeQuality": "cd ./node_modules/code-quality-cli && npm install && npm run start:dev",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "coverageReporters": [
      "text",
      "json-summary"
    ]
  }
}
